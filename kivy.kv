<HomePage>:
    name:'home'
    canvas.before:
        Rectangle:
            size:self.size
            source: 'home.jpg'
        
    
    Button:
        on_release:app.root.current = 'q1'
        size_hint:.7,.2
        pos_hint: {"x":0.15,"y":0.15}
        background_normal:'btn.png'
        background_down:'btn.png'
                  
<Q1>:
    name:'q1'
    BoxLayout:
        orientation: 'vertical'

        Label:
            text:'1. Who developed Python Programming Language?'
            text_size: self.width, None
            size_hint: 1, None
            height: self.texture_size[1]
            font_size:self.width/15
            color: 0,0,0
            canvas.before:
                Rectangle:
                    pos: self.pos
                    size: self.size
            
        Button:
            text: 'a. Wick van Rossum'
            text_size: self.width, None
            font_size:self.width/15
            color: (1,1,0)
            on_release:self.background_color = (255,0.0)
            on_release:self.color = (0,0,0)
        Button:
            text: 'b. Rasmus Lerdorf'
            text_size: self.width, None
            font_size:self.width/15
            color: (1,1,0)
            on_release:self.background_color = (255,0,0)
        Button:
            text: 'c. Guido van Rossum'
            text_size: self.width, None
            font_size:self.width/15
            color: (1,1,0)
            on_release:self.background_color = (0,255,0)
        Button:
            text: 'd. Niene Stom'
            text_size: self.width, None
            font_size:self.width/15
            color: (1,1,0)
            on_release:self.background_color = (255,0,0)
        AnchorLayout:
            anchor_y:'bottom'
            BoxLayout:
                Button:
                    on_release:app.root.current = 'home'
                    background_normal:'backbtn.jpeg'
                    background_down:'backbtn.jpeg'
                Button:
                    on_release:app.root.current = 'q2'
                    background_normal:'nextbtn.jpeg'
                    background_down:'nextbtn.jpeg'

